if True:
    print("debe ejecutarse")
    
if False:
    print("nunca debe ejecutarse")
    
pet = input("¿Cual es tu mascota favorita? ")
if pet =="perro":

    print('genial tienes buen gusto')
    
elif pet =="gato":
    print('genial tambien tienes buen gusto, pero suerte')
    
elif pet =="pez":
    print("Eres lo maximo")
    
else:
    print("Tienes una mascota exótica")
    
